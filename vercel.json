{
    "rewrites": [
        {
            "source": "/api/debug",
            "destination": "/api/debug.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/((?!api/|assets/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}